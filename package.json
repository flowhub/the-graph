    "http-server": "^0.12.3",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "jsjob": "^0.10.13",
    "mocha": "^8.2.1",
    "noflo-canvas": "0.4.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
