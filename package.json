    "webpack": "^5.5.1",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "lint": "eslint --fix index.js render.jsjob.js the-graph-thumb the-graph-nav the-graph-editor",
    "fontawesome": "node scripts/build-font-awesome-javascript.js",
    "stylus": "stylus -c -o dist -m themes/*.styl",
